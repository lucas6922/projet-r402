<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:networkSecurityConfig="@xml/network_security_config"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Parkour"
        tools:targetApi="31">
        <activity
            android:name=".concurrents.view.ListeConcurrents"
            android:exported="false"
            android:label="@string/title_activity_liste_concurrents"
            android:theme="@style/Theme.Parkour" />
        <activity
            android:name=".concurrents.view.Classement"
            android:exported="false"
            android:label="@string/title_activity_classement"
            android:theme="@style/Theme.Parkour" />
        <activity
            android:name=".obstacles.view.ListeObstacles"
            android:exported="false"
            android:label="@string/title_activity_liste_obstacles"
            android:theme="@style/Theme.Parkour" />
        <activity
            android:name=".parkour.view.AjoutParkour"
            android:exported="false"
            android:label="@string/title_activity_ajout_parkour"
            android:theme="@style/Theme.Parkour" />
        <activity
            android:name=".competition.view.AjoutCompetition"
            android:exported="false"
            android:label="@string/title_activity_ajout_competition"
            android:theme="@style/Theme.Parkour" />
        <activity
            android:name=".parkour.view.ListeParkours"
            android:exported="false"
            android:label="@string/title_activity_liste_parkours"
            android:theme="@style/Theme.Parkour" />
        <activity
            android:name=".concurrents.view.InscriptionConcurent"
            android:exported="false"
            android:label="@string/title_activity_inscription_concurent"
            android:theme="@style/Theme.Parkour" />
        <activity
            android:name=".concurrents.view.AjoutConcurrent"
            android:exported="false"
            android:label="Ajout Concurrent"
            android:theme="@style/Theme.Parkour" />
        <activity
            android:name=".obstacles.view.AjoutObstacles"
            android:exported="false"
            android:label="Ajout Obstacle"
            android:theme="@style/Theme.Parkour" />
        <activity
            android:name=".chrono.view.Chronometre"
            android:exported="false"
            android:label="@string/title_activity_chronometre"
            android:theme="@style/Theme.Parkour" />
        <activity
            android:name=".competition.view.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Parkour">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>